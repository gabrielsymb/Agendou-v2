{
  "name": "projeto-agenda-v3-backend",
  "version": "1.0.0",
  "description": "Backend para PWA Agenda v3, Node.js + TypeScript + SQLite",
  "main": "./src/backend/server.ts",
  "scripts": {
    "start": "node ./dist/backend/server.js",
    "dev": "ts-node-dev --respawn --transpile-only ./src/backend/server.ts",
  "seed": "ts-node-dev --respawn --transpile-only ./src/backend/scripts/seed.ts",
  "seed-dates": "ts-node-dev --respawn --transpile-only ./src/backend/scripts/seed-dates.ts",
  "test-backend": "jest --config=jest.config.cjs --runInBand src/backend",
  "set-seed-password": "ts-node-dev --respawn --transpile-only ./src/backend/scripts/set-seed-password.ts",
  "mint-token": "ts-node --transpile-only ./src/backend/scripts/mint-token.ts",
    "build": "tsc",
    "test": "jest --config=jest.config.cjs --runInBand"
  },
  "keywords": [
    "typescript",
    "express",
    "sqlite"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^11.1.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.19",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.0",
    "sqlite3": "^5.1.7",
    "uuid": "^13.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^20.14.9",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3"
  }
}
